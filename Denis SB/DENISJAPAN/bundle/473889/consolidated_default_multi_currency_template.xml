	vertical-align: top; border-right: 1px solid; border-bottom: 1px solid;">
		{ISTEMPLATE_TAXCODERATE}
	</td>
	<td colspan="2" align="center" style="font-weight: bold; text-align: center;
	vertical-align: top; border-right: 1px solid; border-bottom: 1px solid;">
		{ids.label.line_amount}
	</td>
</tr>

<!-- TRANSACTION LINE LOOP Start -->
<#if transactionlist?has_content>
	<!-- customer loop -->
	<#list transactionlist.customers as customer>
	<!-- Display the Customer Name -->
	<tr>
		<td align="left" colspan="13" style="border: 1px solid;">
			${customer.customerName}
		</td>
	</tr>

	<!-- transaction loop -->
	<#list customer.transactions as transaction>
		<tr>
			<td align="left" style="border-left: 1px solid; border-right: 1px solid; border-bottom: 1px solid;">
          <!-- Transaction Date -->
          <#assign tranDate = '${transaction.trandate.value}'>
          ${tranDate?replace("\"", "")}
			</td>
			<td align="right" style="border-right: 1px solid; border-bottom: 1px solid;">
				<!-- Transaction Number -->
				${transaction.transactionnumber.value}
			</td>
			<td colspan="3" align="left" style="border-right: 1px solid; border-bottom: 1px solid;">
				<!-- Item Code -->
				<#if transaction.recordtype.value="salesorder">
					<!-- This is used due to a freemarker + netsuite limitation -->
					${transaction.formulatext.value}
				<#else>
					${transaction.item.text}
				</#if>
			</td>
			<td align="right" style="border-right: 1px solid; border-bottom: 1px solid;">
			<!-- Item Quantity -->
			${transaction.quantity.value}
			</td>
			<td align="left" style="border-right: 1px solid; border-bottom: 1px solid;">
				<!-- Currency -->
				${transaction.currency.text}
			</td>
			<td align="right" style="border-right: 1px solid; border-bottom: 1px solid;">
				<!-- Exchange Rate -->
				${transaction.exchangerate.value?number?string["#,##0.00"]}
			</td>
			<td align="right" style="border-right: 1px solid; border-bottom: 1px solid;">
				<!-- Foreign Amount -->
				${transaction.fxamount.value?number?string["#,##0.00"]}
			</td>
			<td align="right" style="border-right: 1px solid; border-bottom: 1px solid;">
				<!-- Item Price -->
				<#if transaction.rate.value == ''>
					0.00
				<#else>
					${transaction.rate.value?number?string["#,##0.00"]}
				</#if>
			</td>
			<td align="left" style="border-right: 1px solid; border-bottom: 1px solid; text-align:left;">
				<!-- Tax Rate -->
				<#assign rate = '${transaction.rate.taxItem.value?remove_ending("%")}'>
				<#if rate?number == 0>
					{ISTEMPLATE_TAXEXEMPT}
				<#else>
					${rate?number?round}%
				</#if>
			</td>
			<td colspan="2" align="right" style="border-right: 1px solid; border-bottom: 1px solid;">
				<!-- Amount -->
				<#if transaction.type.value?string == 'SalesOrd'>
				0.00
				<#else>
				${transaction.netamountnotax.value?number?string["#,##0.00"]}
			</#if>
			</td>
		</tr>
	</#list>
<!-- TRANSACTION LINE LOOP End -->

	<!-- Add the Subtotal Section per Customer -->
	<#if customer.subtotals?has_content>
		<#assign printedSubtotal = 0>
		<#list customer.subtotals as subtotal>
			<tr>
				<td style="border-left: 1px solid; border-right: 1px solid; border-bottom: 1px solid;"></td>
				<td style="border-right: 1px solid; border-bottom: 1px solid;">
					<!-- we only want the Subtotal Label to appear once per subtotals set-->
					<#if printedSubtotal == 0>
						{CIS_SUBTOTAL}
						<#assign printedSubtotal = 1>
					</#if>
				</td>
				<td colspan="3" style="border-right: 1px solid; border-bottom: 1px solid;"></td>
				<td style="border-right: 1px solid; border-bottom: 1px solid;"></td>
				<td style="border-right: 1px solid; border-bottom: 1px solid;"></td>
				<td style="border-right: 1px solid; border-bottom: 1px solid;"></td>
