<!-- TRANSACTION DETAIL Headers -->
<tr>
	<td align="center" style="font-weight: bold; text-align: center;vertical-align:
	 top; border-left: 1px solid; border-right: 1px solid; border-bottom: 1px solid;">
		{ids.label.tran_date}
	</td>
